<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.nav_view.DashbordFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraint_dashbord"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/DarkPrimaryColor"
            android:paddingBottom="124dp"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:fontFamily="@font/kalameh_bold"
                android:text="صندوق انتظار"
                android:textColor="@color/white"
                android:textSize="26dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"></TextView>

        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.cardview.widget.CardView
            android:id="@+id/cardView"
            android:layout_width="0dp"
            android:layout_height="200dp"
            android:layout_margin="24dp"
            app:cardCornerRadius="18dp"
            app:layout_constraintBottom_toBottomOf="@+id/constraint_dashbord"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraint_dashbord">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/txt_name_dashbord"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/kalameh_bold"
                    android:text="محمدمهدی طاهری"
                    android:textColor="@color/PrimaryText"
                    android:textSize="22sp"></TextView>

                <TextView
                    android:id="@+id/txt_account_number_dashbord"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_name_dashbord"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="12dp"
                    android:fontFamily="@font/kalameh_black"
                    android:text="637902052085"
                    android:textColor="@color/SecondaryText"
                    android:textSize="20dp"></TextView>

                <TextView
                    android:id="@+id/txt_my_payment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:layout_margin="20dp"
                    android:fontFamily="@font/kalameh_black"
                    android:text="5000000 تومان"
                    android:textColor="@color/PrimaryColor"
                    android:textSize="20dp"></TextView>


            </RelativeLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            app:cardBackgroundColor="#FFF29B"
            app:cardCornerRadius="16dp"
            app:layout_constraintEnd_toEndOf="@+id/cardView"
            app:layout_constraintStart_toStartOf="@+id/cardView"
            app:layout_constraintTop_toBottomOf="@+id/cardView">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_margin="8dp"
                    android:background="@drawable/shape_my_score_root"
                    android:padding="8dp"
                    android:src="@drawable/ic_myscore"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="8dp"
                    android:fontFamily="@font/iranyekan_bold"
                    android:text="امتیاز من"
                    android:textColor="@color/PrimaryText"
                    android:textSize="18dp"
                    app:layout_constraintEnd_toStartOf="@+id/imageView2"
                    app:layout_constraintTop_toTopOf="@+id/imageView2" />

                <TextView
                    android:id="@+id/txt_my_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="8dp"
                    android:fontFamily="@font/iranyekan_bold"
                    android:textColor="@color/SecondaryText"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/imageView2"
                    app:layout_constraintEnd_toStartOf="@+id/imageView2"
                    app:layout_constraintTop_toBottomOf="@+id/textView2"
                    tools:text="5 امتیاز" />

                <TextView
                    android:id="@+id/txt_slash"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="4dp"
                    android:fontFamily="@font/iranyekan_bold"
                    android:textColor="@color/SecondaryText"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_my_score"
                    app:layout_constraintEnd_toStartOf="@+id/txt_my_score"
                    app:layout_constraintTop_toTopOf="@+id/txt_my_score"
                    tools:text="/" />

                <TextView
                    android:id="@+id/txt_my_level"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="4dp"
                    android:fontFamily="@font/iranyekan_bold"
                    android:textColor="@color/SecondaryText"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/txt_slash"
                    app:layout_constraintEnd_toStartOf="@+id/txt_slash"
                    app:layout_constraintTop_toTopOf="@+id/txt_slash"
                    tools:text="رتبه 4" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="24dp"
            android:background="@drawable/shape_dashbord"
            android:paddingBottom="70dp"
            app:layout_constraintTop_toBottomOf="@+id/cardView2">

            <View
                android:id="@+id/view"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                app:layout_constraintStart_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"></View>

            <LinearLayout
                android:id="@+id/linearLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="24dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/view">

                <androidx.cardview.widget.CardView
                    android:id="@+id/card_deffeared_installment"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="8dp"
                    android:layout_weight=".4"
                    app:cardCornerRadius="16dp"
                    app:cardElevation="0dp">

                    <androidx.cardview.widget.CardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        app:cardCornerRadius="16dp"
                        app:cardElevation="0dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="160dp"
                            android:background="#C8FF87">

                            <TextView
                                android:id="@+id/txtunit1_in"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/cardunit1_in"
                                android:layout_centerHorizontal="true"
                                android:fontFamily="@font/iranyekan_regular"
                                android:text="اقساط معوقه"
                                android:textColor="@color/black"
                                android:textSize="15sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/txtunit1_in"
                                android:layout_margin="8dp"
                                android:background="@drawable/shape_see_dashbord"
                                android:fontFamily="@font/iranyekan_regular"
                                android:padding="4dp"
                                android:text="مشاهده"
                                android:textColor="@color/black"
                                android:textSize="13sp" />

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardunit1_in"
                                android:layout_width="match_parent"
                                android:layout_height="90dp"
                                app:cardCornerRadius="16dp"
                                app:cardElevation="0dp">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:scaleType="centerCrop"
                                    android:src="@mipmap/installment_payment_pic" />

                            </androidx.cardview.widget.CardView>

                        </RelativeLayout>
                    </androidx.cardview.widget.CardView>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/card_my_payments"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_weight=".6"
                    app:cardCornerRadius="16dp"
                    app:cardElevation="0dp">

                    <androidx.cardview.widget.CardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        app:cardCornerRadius="16dp"
                        app:cardElevation="0dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="160dp"
                            android:background="#7B9AFA">

                            <TextView
                                android:id="@+id/txtunit2_in"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:layout_toEndOf="@+id/cardunit2_in"
                                android:fontFamily="@font/iranyekan_regular"
                                android:gravity="center"
                                android:text="پرداختی های من"
                                android:textColor="@color/black"
                                android:textSize="15sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/txtunit2_in"
                                android:layout_margin="8dp"
                                android:layout_marginTop="16dp"
                                android:layout_toEndOf="@id/cardunit2_in"
                                android:background="@drawable/shape_see_dashbord"
                                android:fontFamily="@font/iranyekan_regular"
                                android:padding="4dp"
                                android:text="مشاهده"
                                android:textColor="@color/black"
                                android:textSize="13sp" />

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardunit2_in"
                                android:layout_width="100dp"
                                android:layout_height="match_parent"
                                app:cardCornerRadius="16dp"
                                app:cardElevation="0dp">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@color/white"
                                    android:src="@mipmap/my_payment" />

                            </androidx.cardview.widget.CardView>

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="24dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout">

                <androidx.cardview.widget.CardView
                    android:id="@+id/card_my_loan"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="8dp"
                    android:layout_weight=".6"
                    app:cardCornerRadius="16dp"
                    app:cardElevation="0dp">

                    <androidx.cardview.widget.CardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        app:cardCornerRadius="16dp"
                        app:cardElevation="0dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="160dp"
                            android:background="#838383">

                            <TextView
                                android:id="@+id/txtunit4_in"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:layout_toEndOf="@+id/cardunit4_in"
                                android:fontFamily="@font/iranyekan_regular"
                                android:gravity="center"
                                android:text="تسهیلات من"
                                android:textColor="@color/black"
                                android:textSize="15sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/txtunit4_in"
                                android:layout_margin="8dp"
                                android:layout_marginTop="16dp"
                                android:layout_toEndOf="@id/cardunit4_in"
                                android:background="@drawable/shape_see_dashbord"
                                android:fontFamily="@font/iranyekan_regular"
                                android:padding="4dp"
                                android:text="مشاهده"
                                android:textColor="@color/black"
                                android:textSize="13sp" />

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardunit4_in"
                                android:layout_width="100dp"
                                android:layout_height="match_parent"
                                app:cardCornerRadius="16dp"
                                app:cardElevation="0dp">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@mipmap/my_loan" />

                            </androidx.cardview.widget.CardView>

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/card_rules"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_weight=".4"
                    app:cardCornerRadius="16dp"
                    app:cardElevation="0dp">

                    <androidx.cardview.widget.CardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        app:cardCornerRadius="16dp"
                        app:cardElevation="0dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="160dp"
                            android:background="#AABFFF">

                            <TextView
                                android:id="@+id/txtunit3_in"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/cardunit3_in"
                                android:layout_centerHorizontal="true"
                                android:fontFamily="@font/iranyekan_regular"

                                android:text="شرایط و ضوابط"
                                android:textColor="@color/black"
                                android:textSize="15sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/txtunit3_in"
                                android:layout_margin="8dp"
                                android:background="@drawable/shape_see_dashbord"
                                android:fontFamily="@font/iranyekan_regular"
                                android:padding="4dp"
                                android:text="مشاهده"
                                android:textColor="@color/black"
                                android:textSize="13sp" />

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardunit3_in"
                                android:layout_width="match_parent"
                                android:layout_height="90dp"
                                app:cardCornerRadius="16dp"
                                app:cardElevation="0dp">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:scaleType="centerCrop"
                                    android:src="@mipmap/img_rules" />

                            </androidx.cardview.widget.CardView>

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                </androidx.cardview.widget.CardView>

            </LinearLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>


